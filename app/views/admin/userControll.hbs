<main role="main">
  
  <section class="panel important">
      <table class="table">
  <thead>
    <tr>
      <th scope="col">Index</th>
      <th scope="col">Tên người dùng</th>
      <th scope="col">Mật khẩu</th>
      <th scope="col">Chức vụ</th>
    </tr>
  </thead>
  <tbody>
      {{#each account}}
    <tr>
      <th scope="row">{{index @index 1}}</th>
      <td>{{this.username}}</td>
      <td>{{this.password}}</td>
      <td>{{this.role}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
  </section>
  

</main>